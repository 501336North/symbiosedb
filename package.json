{
  "name": "symbiosedb",
  "version": "1.0.0",
  "private": true,
  "description": "SymbioseDB: Intelligent data infrastructure combining PostgreSQL + Vectors + Blockchain",
  "workspaces": [
    "packages/*",
    "demos/*",
    "templates/*"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "test:stats": "node scripts/generate-test-stats.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:test": "docker-compose --profile test up -d",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "build": "turbo run build",
    "build:oss": "node scripts/build-oss.js",
    "build:pro": "node scripts/build-pro.js",
    "build:all": "npm run build:oss && npm run build:pro",
    "build:web": "cd packages/design-system && npm run build && cd ../web && npm run build",
    "start:web": "cd packages/web && npm start",
    "dev": "turbo run dev",
    "dev:web": "cd packages/web && npm run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "async": "^3.2.6",
    "compression": "^1.8.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "serve": "^14.2.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@apollo/server": "^5.1.0",
    "@types/inquirer": "^9.0.9",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.0",
    "chalk": "^4.1.2",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "graphql": "^16.12.0",
    "helmet": "^8.1.0",
    "inquirer": "^8.2.6",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  }
}
